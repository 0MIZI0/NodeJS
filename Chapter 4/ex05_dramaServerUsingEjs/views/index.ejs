<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1>My Favorite DramaList</h1>
    <ol>
      <% dramaList.forEach(function(element){ %>
        <li><%=element.title%>(<%=element.actor%>)</li>
      <% }) %>
    </ol>

    <div>-----------------------------------------------</div>

    <form action="/" method="post">
      <p> 제목
        <input type="text" name="title">
      </p>
      <p> 배우
        <input type="text" name="actor">
      </p>
      <input type="submit">
    </form>
  </body>
</html>